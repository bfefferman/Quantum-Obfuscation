@INCOLLECTION{springerlink:10.1007/978-3-642-13858-4_11,
  author = {Reyhanitabar, Mohammad and Susilo, Willy and Mu, Yi},
  title = {Enhanced Security Notions for Dedicated-Key Hash Functions: Definitions
