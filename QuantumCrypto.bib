@INCOLLECTION{springerlink:10.1007/978-3-540-24676-3_19,
  author = {Malkin, Tal and Obana, Satoshi and Yung, Moti},
  title = {The Hierarchy of Key Evolving Signatures and a Characterization of
	Proxy Signatures},
  booktitle = {Advances in Cryptology - EUROCRYPT 2004},
  publisher = {Springer Berlin / Heidelberg},
  year = {2004},
  editor = {Cachin, Christian and Camenisch, Jan},
  volume = {3027},
  series = {Lecture Notes in Computer Science},
  pages = {306-322},
  note = {10.1007/978-3-540-24676-3_19},
  abstract = {For the last two decades the notion and implementations of proxy signatures
	have been used to allow transfer of digital signing power within
	some context (in order to enable flexibility of signers within organizations
	and among entities). On the other hand, various notions of the key-evolving
	signature paradigms (forward-secure, key-insulated, and intrusion-resilient
	signatures) have been suggested in the last few years for protecting
	the security of signature schemes, localizing the damage of secret
	key exposure. In this work we relate the various notions via direct
	and concrete security reductions that are tight. We start by developing
	the first formal model for fully hierarchical proxy signatures, which,
	as we point out, also addresses vulnerabilities of previous schemes
	when self-delegation is used. Next, we prove that proxy signatures
	are, in fact, equivalent to key-insulated signatures. We then use
	this fact and other results to establish a tight hierarchy among
	the key-evolving notions, showing that intrusion-resilient signatures
	and key-insulated signatures are equivalent, and imply forward-secure
	signatures. We also introduce other relations among extended notions.
	Besides the importance of understanding the relationships among the
	various notions that were originally designed with different goals
	or with different system configuration in mind, our findings imply
	new designs of schemes. For example, many proxy signatures have been
	presented without formal model and proofs, whereas using our results
	we can employ the work on key-insulated schemes to suggest new provably
	secure designs of proxy signatures schemes.},
  affiliation = {Columbia University  },
  file = {:C\:\\Users\\huelsing\\Documents\\Literatur\\Malkin (2004) - The Hierarchy of Key Evolving Signatures and a Characterization of Proxy Signatures - full.pdf:PDF;:C\:\\Users\\huelsing\\Documents\\Literatur\\Malkin (2004) - The Hierarchy of Key Evolving Signatures and a Characterization of Proxy Signatures.pdf:PDF},
  owner = {huelsing},
  review = {Proofs about relations among key-evolving notions and Proxy-Signatures.
	Therefore generic constructions to turn one into the other...},
  timestamp = {2010.11.16},
  url = {http://dx.doi.org/10.1007/978-3-540-24676-3_19}
}

@INCOLLECTION{springerlink:10.1007/978-3-540-75670-5_5,
